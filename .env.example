# Example .env file for Monitoring Stack (Docker Swarm)

# Path to configuration files (relative or absolute)
MONITORING_CONFIG_PATH=/home/user/stacks/monitoring

# Grafana credentials
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin
GRAFANA_ALLOW_SIGN_UP=false

# Cloudflared Tunnel
TUNNEL_TOKEN=your_cloudflare_tunnel_token
